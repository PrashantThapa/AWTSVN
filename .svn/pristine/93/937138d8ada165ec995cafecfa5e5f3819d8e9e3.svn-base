using HRFA.ATT;
using HRFA.DataLayer;
using System.Collections.Generic;

namespace HRFA.BLL
{
    public class BLLUserDesignation
    {
        public  List<ATTUserDesignationLoad> GetUserDesignation()
        {
            DLLUserDesignation objDLLUserDesignation = new DLLUserDesignation();
            return objDLLUserDesignation.GetUserDesignation();
        }
        public  ATTUserDesignation GetUserDesignationBYUserId(string userID)
        {
            DLLUserDesignation objDLLUserDesignation = new DLLUserDesignation();
            return objDLLUserDesignation.GetUserDesignationBYUserId(userID);
        }
    }
}
