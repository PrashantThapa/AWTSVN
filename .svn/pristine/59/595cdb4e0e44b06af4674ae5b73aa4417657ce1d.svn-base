using System;

namespace HRFA.ATT
{
    public class ATTPersonQualification
    {
        public ATTPersonQualification()
        {
            _Person = new ATTPerson();
            _Country = new ATTCountry();
            _Qualification = new ATTQualification();
        }

        private ATTPerson _Person;
        public ATTPerson Person
        {
            get { return _Person; }
            set { _Person = value; }
        }

        public string FromDate { get; set; }
        public string ToDate { get; set; }

        public string EntryBy { get; set; }
        public string EntryDate { get; set; }
        public String Status { get; set; }

        public ATTCountry _Country ;
        public ATTCountry Country
        {
            get { return _Country; }
            set { _Country = value; }
        }

        public ATTQualification _Qualification;
        public ATTQualification Qualification
        {
            get { return _Qualification; }
            set { _Qualification = value; }
        }

        public string Title { get; set; }
        public string Institute { get; set; }
        public string Grade { get; set; }
        public Double? Percentage { get; set; }
        public string Remarks { get; set; }
        public string MajorSubject { get; set; }
        public string OptSubject { get; set; }
        public string EduEquivalence { get; set; }
        public string Action { get; set; }

        public Int64? SubmissionNo { get; set; }
        public Int16? SequenceNo { get; set; }
    }
}
