using System.Collections.Generic;

namespace HRFA.ATT
{
    public class ATTModuleFunction
    {
        public ATTModuleFunction()
        {
            _Roles = new List<ATTRole>();
        }
        public string ApplicationID { get; set; }
        public string ModuleID { get; set; }
        public string ModuleDesc { get; set; }
        public string ModuleType { get; set; }
        public string MenuName { get; set; }
        public string McRestricted { get; set; }
        public string FunCD { get; set; }
        public string FunDesc { get; set; }
        public string Action { get; set; }



        public string FromDate { get; set; }
        public string ToDate { get; set; }
        public string UserID { get; set; }
        public string EntryBy { get; set; }
        public string EntryDate { get; set; }
        public string AuthNO { get; set; }
        public string AuthBy { get; set; }
        public string AuthDate { get; set; }



        private List<ATTRole> _Roles ;
        public List<ATTRole> Roles
        {
            get { return _Roles; }
            set { _Roles = value; }
        }


    }
}
