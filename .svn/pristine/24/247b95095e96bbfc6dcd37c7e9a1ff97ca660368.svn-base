using System;
using System.Runtime.Serialization;

namespace HRFA.BLL
{
    [Serializable]
    internal class OracleException : Exception
    {
        public OracleException()
        {
        }

        public OracleException(string message) : base(message)
        {
        }

        public OracleException(string message, Exception innerException) : base(message, innerException)
        {
        }

        protected OracleException(SerializationInfo info, StreamingContext context) : base(info, context)
        {
        }
    }
}